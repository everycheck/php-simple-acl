{
    "name": "everycheck/php-simple-acl",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "symfony/symfony": "3.4.*"
    },
    "require-dev": {
        "php": ">=5.5.9",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "phpunit/phpunit": "^7.0",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "^5.0.0",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "EveryCheck\\SimpleAclBundle\\": "src/"
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "EveryCheck\\SimpleAclBundle\\Tests\\": "Tests"
        }
    }
}
